{
  "resourceType": "StructureDefinition",
  "id": "au-hearing-screen",
  "url": "http://example.org/fhir/StructureDefinition/au-estimated-gestation",
  "version": "v0.0.1",
  "name": "NCDHC Hearing Screen Observation Profile",
  "status": "draft",
  "publisher": "National Children's Digital Health Collaborative-eHNSW",
  "description": "To capture the following details:\r\nHas the hearing check been completed?\r\nOutcome of hearing test in right ear\r\nOutcome of hearing test in left ear\r\nIndication of whether a referral is needed",
  "fhirVersion": "3.0.1",
  "kind": "resource",
  "abstract": false,
  "type": "Observation",
  "baseDefinition": "http://hl7.org/fhir/StructureDefinition/Observation",
  "derivation": "constraint",
  "differential": {
    "element": [
      {
        "id": "Observation",
        "path": "Observation",
        "short": "NCDHC Estimated Gestation Observation Profile",
        "constraint": [
          {
            "key": "ncdhc-obs-1",
            "severity": "error",
            "human": "If there is no component or related element then either a value[x] or a data absent reason must be present",
            "expression": "(component.empty() and related.empty()) implies (dataAbsentReason or value)",
            "xpath": "exists(f:component) or exists(f:related) or exists(f:*[starts-with(local-name(.), 'value')]) or exists(f:dataAbsentReason)"
          }
        ]
      },
      {
        "id": "Observation.status",
        "extension": [
          {
            "url": "http://hl7.org/fhir/StructureDefinition/structuredefinition-display-hint",
            "valueString": "default: final"
          }
        ],
        "path": "Observation.status",
        "defaultValueCode": "final",
        "mustSupport": true
      },
      {
        "id": "Observation.category",
        "path": "Observation.category",
        "min": 1,
        "max": "1",
        "patternCodeableConcept": {
          "coding": [
            {
              "system": "http://hl7.org/fhir/observation-category",
              "code": "TBD",
              "display": "TBD"
            }
          ],
          "text": "This needs to be discussed and finalized"
        },
        "mustSupport": true
      },
      {
        "id": "Observation.code",
        "path": "Observation.code",
        "short": "LOINC Code to represent Gestational age at birth",
        "patternCodeableConcept": {
          "coding": [
            {
              "system": "http://snomed.info/sct",
              "version": "AU - 20181130",
              "code": "417491009",
              "display": "Neonatal hearing test"
            }
          ]
        },
        "mustSupport": true,
        "binding": {
          "extension": [
            {
              "url": "http://hl7.org/fhir/StructureDefinition/elementdefinition-bindingName",
              "valueString": "ObservationCode"
            }
          ],
          "strength": "preferred"
        }
      },
      {
        "id": "Observation.subject",
        "path": "Observation.subject",
        "short": "Reference to the Mother Patient",
        "min": 1,
        "type": [
          {
            "code": "Reference",
            "targetProfile": "http://hl7.org.au/fhir/StructureDefinition/au-patient"
          }
        ],
        "mustSupport": true
      },
      {
        "id": "Observation.effective[x]:effectiveDateTime",
        "path": "Observation.effectiveDateTime",
        "sliceName": "effectiveDateTime",
        "type": [
          {
            "code": "dateTime"
          }
        ]
      },
      {
        "id": "Observation.performer",
        "path": "Observation.performer",
        "type": [
          {
            "code": "Reference",
            "targetProfile": "http://hl7.org.au/fhir/StructureDefinition/au-organisation"
          },
          {
            "code": "Reference",
            "targetProfile": "http://hl7.org.au/fhir/base2018Oct/StructureDefinition/au-patient"
          },
          {
            "code": "Reference",
            "targetProfile": "http://hl7.org.au/fhir/StructureDefinition/au-relatedperson"
          },
          {
            "code": "Reference",
            "targetProfile": "http://hl7.org.au/fhir/StructureDefinition/au-practitioner"
          }
        ]
      },
      {
        "id": "Observation.value[x]:valueCodeableConcept",
        "path": "Observation.valueCodeableConcept",
        "sliceName": "valueCodeableConcept",
        "short": "Indication of whether a referral is needed",
        "type": [
          {
            "code": "CodeableConcept"
          }
        ],
        "patternCodeableConcept": {
          "coding": [
            {
              "system": "http://snomed.info/sct",
              "version": "AU - 20181130",
              "code": "373066001",
              "display": "Yes"
            },
            {
              "system": "http://snomed.info/sct",
              "version": "AU - 20181130",
              "code": "373067005",
              "display": "No"
            }
          ]
        },
        "mustSupport": true
      },
      {
        "id": "Observation.comment",
        "path": "Observation.comment",
        "comment": "The server may represent the input Quantity into weeks and days"
      },
      {
        "id": "Observation.component:hearing-creen-outcome",
        "path": "Observation.component",
        "sliceName": "hearing-creen-outcome",
        "slicing": {
          "discriminator": [
            {
              "type": "value",
              "path": "code"
            }
          ],
          "rules": "open"
        },
        "short": "Outcome Left",
        "min": 1,
        "max": "2"
      },
      {
        "id": "Observation.component:hearing-creen-outcome:outcome-left",
        "path": "Observation.component",
        "sliceName": "outcome-left",
        "max": "1"
      },
      {
